type Dir = enum { North, South, East, West } in

let expr = (Dir of South, 2, struct {x=5, y=-6}) in
    match expr {
        (of North, _, _)
        | (of East, _, _)
        | (of West, _, _)
        | (of South, 3, _) => put "Incorrect!\n",
        (of South, 2, struct {x=a, y=b}) => {
            if (a == 5 and b == -6) {
                put "Correct!\n"
            } else {
                put "Incorrect!\n"
            }
        },
        _ => put "Default! (incorrect)\n"
    }