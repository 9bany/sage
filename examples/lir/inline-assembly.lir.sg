proc asm_init() = core {
    fun FACT
        set A, 0
        gt [SP], A, B
        if B
            push [SP]
            dec [SP]
            call FACT
            mul [SP], [SP - 1]
            pop
        else
            set [SP], 1
        end
    end
} in

proc fact(n: Int) -> Int = core {
    call FACT
} in {
    asm_init();

    put fact(5); put '\n';
    put fact(10); put '\n';
}