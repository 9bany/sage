type Option = let T = Point in enum {
    of Some T,
    of Nothing 
}, Point = struct { x: Int, y: Int } in


let val = struct { x = 5, y = 6 } as Option of Some
    in match val {
        of Some struct {x=a, y=b} => {
            put a;
            put ", ";
            put b;
        },
        of Nothing None => {
            put "Nothing";
        }
    }